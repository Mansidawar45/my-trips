{
  "kind": "collectionType",
  "collectionName": "travel_guides",
  "info": {
    "singularName": "travel-guide",
    "pluralName": "travel-guides",
    "displayName": "TravelGuide"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Content": {
      "type": "blocks"
    },
    "coverimage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "AuthorName": {
      "type": "string"
    },
    "PublishDate": {
      "type": "date"
    },
    "destination": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::destination.destination",
      "inversedBy": "travel_guides"
    }
  }
}
