{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:production": "rm -rf out && next build",
    "start": "next start",
    "preview": "npx serve out -p 3000",
    "lint": "eslint",
    "export": "next export",
    "deploy:s3": "npm run build:production && aws s3 sync out/ s3://your-bucket-name --delete",
    "deploy:cloudfront": "aws cloudfront create-invalidation --distribution-id YOUR_DISTRIBUTION_ID --paths '/*'"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "lucide-react": "^0.553.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "autoprefixer": "^10.4.22",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14"
  }
}
